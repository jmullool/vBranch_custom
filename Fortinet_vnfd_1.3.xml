<config xmlns="http://tail-f.com/ns/config/1.0">
  <nfvo xmlns="http://tail-f.com/pkg/tailf-etsi-rel2-nfvo">
  <vnfd>
    <id>Fortinet</id>
    <product-info-description>Fortinet</product-info-description>
    <vdu>
      <id>fortinet</id>
      <internal-connection-point-descriptor>
        <id>mgmt</id>
        <external-connection-point-descriptor>mgmt</external-connection-point-descriptor>
        <layer-protocol>IPv4</layer-protocol>
        <interface-id xmlns="http://tail-f.com/pkg/tailf-etsi-rel2-nfvo-nfvis">0</interface-id>
      </internal-connection-point-descriptor>
      <virtual-compute-descriptor>fortinet-virtual-compute</virtual-compute-descriptor>
      <virtual-storage-descriptor>fortinet-virtual-storage</virtual-storage-descriptor>
      <software-image-descriptor>
        <version></version>
        <checksum></checksum>
        <container-format>bare</container-format>
        <disk-format>qcow2</disk-format>
        <image>http://10.87.163.152/images/Fortigate-NFVIS.tar.gz</image>
        <image-name xmlns="http://tail-f.com/pkg/tailf-etsi-rel2-nfvo-nfvis">fortinet</image-name>
      </software-image-descriptor>
      <device-type xmlns="http://tail-f.com/pkg/tailf-etsi-rel2-nfvo-nfvis">
        <cli>
          <ned-id>fortinet-fortios-id:fortinet-fortios</ned-id>
        </cli>
        <port>22</port>
      </device-type>
      <day0 xmlns="http://tail-f.com/pkg/tailf-etsi-rel2-nfvo-nfvis">
        <destination>/openstack/latest/user_data</destination>
      </day0>
    </vdu>
    <virtual-compute-descriptor>
      <id>fortinet-virtual-compute</id>
      <virtual-memory>
        <virtual-memory-size>4.0</virtual-memory-size>
      </virtual-memory>
      <virtual-cpu>
        <number-of-virtual-cpus>2</number-of-virtual-cpus>
      </virtual-cpu>
    </virtual-compute-descriptor>
    <virtual-storage-descriptor>
      <id>fortinet-virtual-storage</id>
      <type-of-storage>root</type-of-storage>
      <size-of-storage>8</size-of-storage>
    </virtual-storage-descriptor>
    <virtual-link-descriptor>
      <id>mgmt</id>
      <connectivity-type>
        <layer-protocol>IPv4</layer-protocol>
      </connectivity-type>
    </virtual-link-descriptor>
    <external-connection-point-descriptor>
      <id>cp-ext</id>
      <layer-protocol>IPv4</layer-protocol>
    </external-connection-point-descriptor>
    <external-connection-point-descriptor>
      <id>cp-int</id>
      <layer-protocol>IPv4</layer-protocol>
    </external-connection-point-descriptor>
    <external-connection-point-descriptor>
      <id>mgmt</id>
      <virtual-link-descriptor>mgmt</virtual-link-descriptor>
      <layer-protocol>IPv4</layer-protocol>
      <management xmlns="http://tail-f.com/pkg/tailf-etsi-rel2-nfvo-nfvis"/>
    </external-connection-point-descriptor>
    <deployment-flavor>
      <id>Fortinet</id>
      <vdu-profile>
        <vdu>fortinet</vdu>
        <min-number-of-instances>1</min-number-of-instances>
        <max-number-of-instances>1</max-number-of-instances>
      </vdu-profile>
      <instantiation-level>
        <id>Fortinet</id>
        <vdu-level>
          <vdu>fortinet</vdu>
          <number-of-instances>1</number-of-instances>
        </vdu-level>
      </instantiation-level>
      <default-instantiation-level>Fortinet</default-instantiation-level>
    </deployment-flavor>
  </vnfd>
  </nfvo>
</config>
